{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "jsxImportSource": "@emotion/react",
    "baseUrl": "./src",
    "paths": {
      "@apis/*": ["apis/*"],
      "@atoms/*": ["atoms/*"],
      "@constants/*": ["constants/*"],
      "@enums/*": ["enums/*"],
      "@hooks/*": ["hooks/*"],
      "@libraries/*": ["libraries/*"],
      "@models/*": ["models/*"],
      "@shared/*": ["shared/*"],
      "@home/*": ["home/*"],
      "@mypage/*": ["mypage/*"],
      "@search/*": ["search/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
