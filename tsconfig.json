{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "jsxImportSource": "@emotion/react",
    "baseUrl": "./src",
    "paths": {
      "@home/*": ["home/*"],
      "@auth/*": ["auth/*"],
      "@mypage/*": ["mypage/*"],
      "@places/*": ["places/*"],
      "@enroll/*": ["places/enroll/*"],
      "@search/*": ["places/search/*"],
      "@place/*": ["places/place/*"],
      "@guide/*": ["plan/guide/*"],
      "@list/*": ["plan/list/*"],
      "@pages/*": ["../pages/*"],
      "@shared/*": ["shared/*"]
    },
    "types": ["kakao.maps.d.ts"]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
