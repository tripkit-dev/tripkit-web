{
  "semi": false,
  "singleQuote": true,
  "trailingComma": "none",
  "importOrder": [
    ".s?css$",
    ".styled$",
    "^react",
    "^next",
    "^@enums/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@components/(.*)$",
    "^[./]"
